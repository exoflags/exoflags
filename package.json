{
  "name": "exoflags",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://exoflags.github.io",
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "@reach/router": "^1.2.1",
    "arup-scripts": "0.1.5",
    "csvtojson": "^2.0.10",
    "d3-array": "^2.3.1",
    "d3-axis": "^1.0.12",
    "d3-drag": "^1.2.4",
    "d3-scale": "^3.0.1",
    "d3-selection": "^1.4.0",
    "emotion-theming": "^10.0.14",
    "gh-pages": "^2.1.1",
    "hash-source": "^1.0.4",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-select": "^3.0.4",
    "react-virtualized-auto-sizer": "^1.0.2",
    "typography": "^0.16.19"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -b master -r https://github.com/exoflags/exoflags.github.io.git",
    "start": "arup-scripts start",
    "build": "arup-scripts build",
    "test": "arup-scripts test",
    "eject": "arup-scripts eject",
    "prettier": "prettier --config prettier.config.js --write \"./**/*.js\"",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "merge-data": "node src/scripts/mergeData.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^3.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "source-map-explorer": "^2.0.1"
  }
}
